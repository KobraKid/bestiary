{
  "name": "bestiary",
  "version": "1.0.0",
  "description": "Bestiary - flexible and customizable",
  "main": "index.js",
  "scripts": {
    "start": "chcp 65001 && npm run build && concurrently \"npm:electron\" \"npm:watch\" -k -c \"yellow,gray\"",
    "build": "webpack --config ./webpack.config.js",
    "electron": "electron ./dist/index.js",
    "watch": "webpack --config ./webpack.config.js --watch",
    "package": "npm run build && copy package.json dist\\package.json && npx electron-packager ./dist Bestiary --overwrite --icon=icon-light.ico",
    "prod": ".\\bestiary-win32-x64\\bestiary.exe"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "keywords": [],
  "author": "KobraKid",
  "license": "ISC",
  "devDependencies": {
    "@types/fparser": "^2.0.0",
    "@types/node": "^17.0.45",
    "@types/react": "^17.0.62",
    "@types/react-dom": "^17.0.20",
    "@types/react-transition-group": "^4.4.4",
    "css-loader": "^6.7.1",
    "electron": "^25.2.0",
    "electron-packager": "^15.4.0",
    "html-webpack-plugin": "^5.5.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "sass": "^1.52.2",
    "sass-loader": "^13.0.0",
    "style-loader": "^3.3.1",
    "ts-loader": "^9.4.4",
    "typescript": "^4.9.5",
    "webpack": "^5.88.1",
    "webpack-cli": "^4.9.2"
  },
  "dependencies": {
    "@hedgedoc/html-to-react": "^2.0.0",
    "chalk": "^5.0.1",
    "cheerio": "^1.0.0-rc.12",
    "concurrently": "^7.2.1",
    "env-paths": "^3.0.0",
    "fparser": "^2.0.2",
    "mongodb": "^5.6.0",
    "mongoose": "^7.3.1",
    "node-sass": "^7.0.3",
    "react-indiana-drag-scroll": "^2.2.0",
    "react-transition-group": "^4.4.2"
  }
}
